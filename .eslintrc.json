{
  "extends": "next/core-web-vitals",
  "plugins": ["import", "simple-import-sort", "unused-imports"],
  "rules": {
      "semi": ["error", "always"],
      "quotes": ["error", "single"],
      "no-empty": [
          "error",
          {
              "allowEmptyCatch": true
          }
      ],
      "import/first": "error",
      "simple-import-sort/exports": "error",
      "simple-import-sort/imports": "error",
      "unused-imports/no-unused-imports": "error"
  }
}
